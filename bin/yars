#!/usr/bin/perl

# PODNAME: yars
# ABSTRACT: Yet Another RESTful-Archive Service
our $VERSION = '0.85'; # VERSION


use Yars;
use Clustericious::Commands;

use strict;
use warnings;

$ENV{MOJO_APP} ||= 'Yars';

&main;

sub main {
    my $cmd = $ARGV[0];
    my $app = Yars->new();
    $app = Clustericious::Commands->start;
    $app;
}



__END__
=pod

